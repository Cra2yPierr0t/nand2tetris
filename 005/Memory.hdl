CHIP Memory {
        IN in[16],load,address[15];
        OUT out[16];

        PARTS:
        RAM16K(in=in,address[0..13],load=load,out=w1);
        Screen(in=in,address[0..12],load=load,out=w2);
        Keyboard(out=w3);

        Mux16(a=w2,b=w3,sel=address[13],out=w4);
        Mux16(a=w1,b=w4,sel=address[14],out=out);
    }
